{
    "name": "qUIck-React",
    "version": "0.0.1",
    "description": "A component library for creating websites in very fast-paced and simple manner",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
    },
    "author": "GDSC",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.6.5",
        "@types/react": "^18.2.22",
        "@types/react-dom": "^18.2.7",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^5.2.2"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "dependencies": {
        "node": "^20.6.0",
        "react-router-dom": "^6.17.0",
        "styled-components": "^6.1.0",
        "switch": "^0.0.0"
    }
}
